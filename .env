PORT=3000

# OAuth2 client (this is the “key” you hand out)
OAUTH_CLIENT_ID=gunny-client
OAUTH_CLIENT_SECRET=SuperSecret123!

# JWT settings (change this to a long random string when you’re ready)
JWT_SECRET=GunnyBot_JWT_Change_This_To_Something_Long_And_Random
JWT_EXPIRES_IN=300

# LLaMA server
JETSON_LLM_URL=http://127.0.0.1:8080/v1/chat/completions
JETSON_LLM_MODEL=Mistral-7B-Instruct-v0.3.Q4_K_M.gguf

# Logging
LOG_LEVEL=info

# CORS – allow specific origins (comma-separated), or leave empty for now
ALLOWED_ORIGINS=http://localhost,http://127.0.0.1

# Rate limiting (home lab defaults)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60
